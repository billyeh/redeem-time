{
  "name": "Redeem the Time",
  "description": "Stay focused by taking God's grace as your supply",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs"],

  "background": {
    "scripts": ["assets/js/background.js"],
    "persistent": true
  }, 

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "action.html"
  },

  "options_page": "options.html"
}
